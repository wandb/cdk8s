{
  "name": "cdk8s",
  "version": "1.0.0",
  "main": "bin/main.js",
  "bin": "bin/main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "gen": "ts-node main.ts",
    "gen:schema": "ts-node src/schema.ts",
    "synth": "cdk8s synth",
    "compile": "tsc --build",
    "watch": "tsc --build -w",
    "test": "jest",
    "build:cdk": "npm run compile && npm run test && npm run synth",
    "build": "tsc",
    "upgrade": "npm i cdk8s@latest cdk8s-cli@latest",
    "upgrade:next": "npm i cdk8s@next cdk8s-cli@next"
  },
  "dependencies": {
    "@colors/colors": "^1.5.0",
    "cdk8s": "^2.7.77",
    "cdk8s-plus-26": "^2.4.17",
    "constructs": "^10.2.46",
    "js-yaml": "^4.1.0",
    "triple-beam": "^1.3.0",
    "uuid": "^9.0.0",
    "winston": "^3.9.0",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.2.5",
    "cdk8s-cli": "^2.2.55",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
